{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_BACKEND_URL": "@vite_backend_url",
    "VITE_BACKEND_GRAPHQL_URL": "@vite_backend_graphql_url"
  },
  "build": {
    "env": {
      "VITE_API_URL": "@vite_api_url",
      "VITE_BACKEND_URL": "@vite_backend_url",
      "VITE_BACKEND_GRAPHQL_URL": "@vite_backend_graphql_url"
    }
  }
}